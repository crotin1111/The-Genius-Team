<!DOCTYPE html>
<html lang="en">
<%- include('header') %>

<body>
  <div class='container'>
    <form id="frm1" action="/action_page.php" style=>
      <strong>Create your WoW account</strong>
      <hr>
      <label for='accName'>Username:</label><br>
      <input class='form-control' id="accName" type="text" name="accName" /><br>
      <label for='accPass'>Password:</label><br>
      <input class='form-control' id='accPass' type="password" name="accPass" required /><br>
      <label for='accEmail'>Account Email:</label><br>
      <input class='form-control' id='accEmail' type="email" name="accEmail" required /><br>

      <input class='btn btn-primary' id='submit' type="button" onclick="submitAccount()" value="Register" />
    </form>

    <%- include('script') %>
    <%- include('footer') %>

  </div>
</body>

</html>
